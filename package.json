{
	"name": "intee",
	"version": "0.2.1",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.12.0",
	"scripts": {
		"build": "yarn workspaces foreach -A --no-private -p run build",
		"publish": "yarn build && yarn workspaces foreach -A --no-private npm publish --tolerate-republish --access public",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"prettier": "^3.7.4",
		"tsdown": "^0.18.2",
		"typescript": "^5.9.3",
		"vitest": "^2.1.9"
	},
	"workspaces": [
		"./packages/*",
		"./examples/*"
	],
	"dependencies": {
		"effect": "^3.19.13"
	}
}
